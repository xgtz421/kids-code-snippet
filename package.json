{
  "name": "kids-code-snippet",
  "publisher": "BrucePi", 
  "displayName": "kids-code-snippet",
  "description": "",
  "icon": "images/icon.png",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/js-common.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/ts-common.json"
      },
      {
        "language": "html",
        "path": "./snippets/html-common.json"
      },
      {
        "language": "less",
        "path": "./snippets/less-common.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xgtz421/kids-code-snippet.git"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "package": "vsce package",
    "publish": "vsce publish",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
