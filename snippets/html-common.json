    "AngularJS select element Wrapper(value is object)": {
        "scope": "html",
        "prefix": "my-select-selected-object",
        "body": [
            "<select",
            "    class=\"form-control\"",
            "    ng-model=\"ctrl.selectedPermissionGroup\"",
            "    ng-options=\"option.label for option in ctrl.permissionGroups\"",
            "></select>"
        ],
        "description": "Select元素(选中值为对象)"
    },

    "AngularJS select element Wrapper(value is id)": {
        "scope": "html",
        "prefix": "my-select-selected-id",
        "body": [
            "<select",
            "    class=\"form-control\"",
            "    ng-model=\"ctrl.selectedGroupId\"",
            "    ng-options=\"option.value as option.label for option in ctrl.permissionGroups\"",
            "></select>"
        ],
        "description": "Select元素(选中值为id)"
    },

    "AngularJS中自定义select的option-label": {
        "scope": "html",
        "prefix": "my-select-option-label",
        "body": [
            "<select",
            "    class=\"form-control\"",
            "    ng-model=\"ctrl.selectedPermissionGroup\"",
            "    ng-options=\"option as (option.code | NumberLeftPad0:2) + '\u3000' + option.label for option in ctrl.permissionGroups\"",
            "    required",
            "></select>"
        ],
        "description": "AngularJS中自定义select的option-label"
    },


